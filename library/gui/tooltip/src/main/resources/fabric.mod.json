{
  "schemaVersion": 1,
  "id": "quilt_tooltip",
  "name": "Quilt Tooltip API",
  "version": "${version}",
  "environment": "*",
  "license": "Apache-2.0",
  "icon": "assets/quilt_tooltip/icon.png",
  "contact": {
    "homepage": "https://quiltmc.org",
    "issues": "https://github.com/QuiltMC/quilt-standard-libraries/issues",
    "sources": "https://github.com/QuiltMC/quilt-standard-libraries"
  },
  "authors": [
    "QuiltMC"
  ],
  "depends": {
    "fabricloader": ">=0.12",
    "minecraft": ">=1.18.2-alpha.22.3.a",
    "quilt_base": "*"
  },
  "description": "Hooks and API to manipulate tooltips.",
  "entrypoints": {
    "client_events": [
      "org.quiltmc.qsl.tooltip.impl.client.QuiltClientTooltipMod"
    ]
  },
  "mixins": [
    "quilt_tooltip.mixins.json"
  ],
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ],
      "parent": {
        "id": "qsl",
        "name": "Quilt Standard Libraries",
        "description": "A set of libraries to assist in making Quilt mods.",
        "icon": "assets/quilt_tooltip/icon.png",
        "badges": [
          "library"
        ]
      }
    }
  }
}

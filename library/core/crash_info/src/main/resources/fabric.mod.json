{
  "schemaVersion": 1,
  "id": "quilt_crash_info",
  "name": "Quilt Crash Info",
  "version": "${version}",
  "environment": "*",
  "license": "Apache-2.0",
  "icon": "assets/quilt_crash_info/icon.png",
  "contact": {
    "homepage": "https://quiltmc.org",
    "issues": "https://github.com/QuiltMC/quilt-standard-libraries/issues",
    "sources": "https://github.com/QuiltMC/quilt-standard-libraries"
  },
  "authors": [
    "QuiltMC"
  ],
  "entrypoints": {
    "events": [
      "org.quiltmc.qsl.crash.impl.CrashInfoImpl"
    ]
  },
  "depends": {
    "fabricloader": ">=0.12",
    "minecraft": ">=1.18.2",
    "quilt_base": "*"
  },
  "description": "Adds information about the Quilt environment in crash reports, and provides an API for other mods to add their own information.",
  "mixins": [
    "quilt_crash_info.mixins.json"
  ],
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ],
      "parent": {
        "id": "qsl",
        "name": "Quilt Standard Libraries",
        "description": "A set of libraries to assist in making Quilt mods.",
        "icon": "assets/quilt_crash_info/icon.png",
        "badges": [
          "library"
        ]
      }
    }
  }
}
